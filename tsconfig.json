{
    "compilerOptions":{
        "target": "ESNext",
        "allowJs": true,
        "skipLibCheck":true,
        "noEmit": true,
        "module":"ESNext",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "strict": true,
        "baseUrl": ".",
        "paths": {
            "@/*":["src/*"]
        }
       
    },
     
     "include":[
        "src/**/*.ts",
         "src/**/*.d.ts",
    ],
    "exclude":[
        "node_modules"
    ]
   
}